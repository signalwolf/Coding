# can covered by some example, not worth to have algorithm 

class Solution(object):
    def canWinNim(self, n):
        """
        :type n: int
        :rtype: bool
        """
        # dp n = 1, 2, 3 w; 4 l; 5 - 1 W; 6 - 2; W; 7 -3; W; 8 - 1 = 7, 8 - 2 = 6, 8-3 = 5 L; 9 12 - 1 - 3
        return not (n%4 == 0)
