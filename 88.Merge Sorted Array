# run time: 56ms, 18%; 42ms, 62%; 49ms, 33%
class Solution(object):
    def merge(self, nums1, m, nums2, n):
        """
        :type nums1: List[int]
        :type m: int
        :type nums2: List[int]
        :type n: int
        :rtype: void Do not return anything, modify nums1 in-place instead.
        """
        i1, i2, i3 = m - 1, n - 1, m + n - 1
        while i1 >= 0 and i2 >= 0:
            if nums1[i1] >= nums2[i2]:
                nums1[i3] = nums1[i1]
                i1 -= 1
                i3 -= 1
            else:
                nums1[i3] = nums2[i2]
                i2 -= 1
                i3 -= 1
        if i2 >= 0:
            nums1[:i2 + 1] = nums2[:i2 + 1]
        
        
        
